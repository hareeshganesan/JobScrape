<script type="text/javascript">
var showStick= false ;
function showSticky(){
	if(RecommedationUtil.getCookie("expSticky")!="true"){
	  if(showStick){
		 var popleft = null;
		 if (document.all)
		 popleft=(document.body.clientWidth/2)-440;
		 else
		 popleft=((document.documentElement.clientWidth-760)/2)-60;
		 var popwin=$('erpop');
		 new Effect.Move(popwin, { x: popleft, y: 130, mode: 'absolute' });
		//     Effect.Appear('erpop', { duration: 1.5 });
		//     $('erpop').show();
	}
   }
}
function hideSticky(){
  $('erpop').hide()
  if($('noshow').checked){
    var expireDate = new Date;
    expireDate.setFullYear(2050)
    RecommedationUtil.createCookie("expSticky", "true", expireDate)
  }
}



	loadAllChicklets();
	Event.observe(window, 'load', function() {
		showSticky();
	});
</script>

